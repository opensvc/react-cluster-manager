    "react-router-dom": "^5.2.0",
    "react-sparklines": "^1.7.0",
    "sass-loader": "^9.0.0",
    "stream-http": "^3.2.0",
    "style-loader": "^0.23.1",
    "typeface-roboto": "0.0.54",
    "url": "^0.11.3",
    "url-loader": "^3.0.0",
